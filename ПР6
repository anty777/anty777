#include <stdio.h>
#include <math.h>

double визначити_довжину_вектора(int х1, int у1, int х2, int у2) {
    double довжина = sqrt(pow(х2 - х1, 2) + pow(у2 - у1, 2));
    return довжина;
}

int main() {
    int х1, у1, х2, у2;
    printf("Введіть координати початку вектора (х1, у1): ");
    scanf("%d %d", &х1, &у1);
    printf("Введіть координати кінця вектора (х2, у2): ");
    scanf("%d %d", &х2, &у2);

    double довжина_вектора = визначити_довжину_вектора(х1, у1, х2, у2);
    printf("Довжина вектора: %.6lf\n", довжина_вектора);

    return 0;
}
